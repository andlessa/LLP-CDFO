! ==================
! General parameters 
! ==================
Init:showChangedSettings = on     ! list changed settings
Init:showChangedParticleData = off ! list changed particle data
Init:showProcesses = on
Init:showMultipartonInteractions = off
Next:numberCount = 15000             ! print message every n events
Next:numberShowInfo = 1            ! print event information n times
Next:numberShowProcess = 1         ! print process record n times
Next:numberShowEvent = 1           ! print event record n times
Next:numberShowLHA  = 0
Stat:showProcessLevel = on
Stat:showErrors = on
Check:epTolErr = 1.00e-2
Main:timesAllowErrors = 100


JetMatching:doShowerKt   = off
JetMatching:etaJetMax=1.00e+03
JetMatching:scheme=1
JetMatching:setMad=off
JetMatching:coneRadius=1.00e+00
JetMatching:nQmatch=4

!
! ==========================
! User customized parameters 
! ==========================
!
Next:numberShowInfo = 1            ! print event information n times
Next:numberShowProcess = 1         ! print process record n times
Next:numberShowEvent = 1           ! print event record n times
RHadrons:allow = on
RHadrons:allowDecay = on
RHadrons:probGluinoball = 0.1


Tune:pp = 21 ! A14 tune with NNPDF2.3LO

ParticleDecays:limitTau = on
ParticleDecays:tauMax = 15000  ! All particles with (lab) lifetime smaller than 15m are decayed
